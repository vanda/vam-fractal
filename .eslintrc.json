{
  "env": {
    "browser": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": { 
    "requireConfigFile" : false
  },
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": "off",
    "prefer-destructuring": ["error", 
      {
        "object": false, 
        "array": false
      }
    ],
    "no-param-reassign": ["error", 
      { 
        "props": false 
      }
    ]
  }
}
